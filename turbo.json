{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"dev": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "build/**"],
			"env": ["NODE_ENV", "NEXT_PUBLIC_*", "EXPO_PUBLIC_*", "API_URL"]
		},
		"lint": {
			"outputs": []
		},
		"typecheck": {
			"outputs": []
		},
		"test": {
			"outputs": ["coverage/**"]
		},
		"deploy:web": {
			"cache": false
		},
		"deploy:api": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	}
}