{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "test": {
      "cache": true,
      "outputs": [],
      "inputs": ["$TURBO_DEFAULT$", "__tests__/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "build/**"]
    },
    "dev": { "cache": false },
    "lint": {},
    "typecheck": {}
  }
}