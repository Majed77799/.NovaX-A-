{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "build/**"],
      "env": ["TURBO_REMOTE_CACHE_SIGNATURE", "NEXT_IMAGE_UNOPTIMIZED"],
      "inputs": ["$TURBO_DEFAULT", "!**/*.md", "!**/*.spec.*", "!**/__tests__/**"]
    },
    "dev": { "cache": false },
    "lint": { "outputs": [] },
    "typecheck": { "outputs": [] }
  }
}