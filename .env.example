# Copy this file to .env and fill in the values. Do NOT commit .env to version control.
# Provider API keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=anthropic-...
GOOGLE_API_KEY=google-...

# Supabase
SUPABASE_URL=https://YOUR-PROJECT.supabase.co
SUPABASE_ANON_KEY=...
SUPABASE_SERVICE_ROLE=...

# Payments
STRIPE_SECRET=sk_live_...
# Example: {"skuA":"prod_A","skuB":"prod_B"}
GUMROAD_PRODUCT_MAP_JSON={"SKU":"PRODUCT_ID"}

# Web Push (VAPID)
VAPID_PUBLIC_KEY=...
VAPID_PRIVATE_KEY=...

# App settings
# 0 disables mocks, 1 enables mocks
NOVAX_ENABLE_MOCKS=0
# choose: openai | anthropic | google
NOVAX_DEFAULT_PROVIDER=openai