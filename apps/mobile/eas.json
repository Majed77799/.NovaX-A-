{
	"cli": {
		"version": ">= 13.0.0"
	},
	"build": {
		"development": {
			"channel": "development",
			"android": {
				"gradleCommand": ":app:assembleDebug"
			},
			"ios": {
				"buildConfiguration": "Debug"
			}
		},
		"preview": {
			"channel": "preview",
			"android": {
				"buildType": "apk"
			},
			"ios": {
				"simulator": true
			}
		},
		"production": {
			"channel": "production",
			"autoIncrement": true,
			"android": {
				"buildType": "app-bundle"
			},
			"ios": {
				"autoIncrement": true
			}
		}
	},
	"submit": {
		"production": {}
	}
}