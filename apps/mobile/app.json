{
	"expo": {
		"name": "Ello",
		"slug": "ello-replica",
		"scheme": "ello",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": { "image": "./assets/splash.png", "resizeMode": "contain", "backgroundColor": "#ffffff" },
		"updates": { "fallbackToCacheTimeout": 0 },
		"assetBundlePatterns": ["**/*"],
		"runtimeVersion": { "policy": "appVersion" },
		"extra": { "apiUrl": "https://example.com" },
		"ios": { "supportsTablet": true, "bundleIdentifier": "com.novax.ello", "buildNumber": "1", "jsEngine": "hermes", "infoPlist": { "ITSAppUsesNonExemptEncryption": false } },
		"android": { "package": "com.novax.ello", "versionCode": 1, "adaptiveIcon": { "foregroundImage": "./assets/adaptive-icon.png", "backgroundColor": "#FFFFFF" }, "jsEngine": "hermes", "permissions": ["INTERNET"] },
		"plugins": [["expo-build-properties", { "android": { "enableProguardInReleaseBuilds": true, "enableShrinkResourcesInReleaseBuilds": true } }]]
	}
}